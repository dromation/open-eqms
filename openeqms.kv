

MDBoxLayout:
    toolbar: menubar
    screen_manager: sm
    size: root.width, root.height
    orientation: "vertical"
    #pos_hint: {'center_x': 0.0, 'center_y': 0.0}
    #adaptive_height: True
    #md_bg_color: app.theme_cls.primary_color
    #MDBoxLayout:
        #orientation: "horizontal"
        #size_hint: 1, 0.1
        #pos_hint: {"center_x": .5, "center_y": 0.9}

    MDBoxLayout:
        id: menubar
        pos_hint: {'center_x': .5}
        adaptive_height: True
        #adaptive_width: True
        halign: "top"
        
        #size_hint_x: 1
        mb_bg_color: app.theme_cls.primary_color
        MDIconButton:
            adaptive_height: True
            adaptive_width: True
            icon: "home"
            md_bg_color: menubar.md_bg_color
            on_release: sm.current = 'HOME'

        MDIconButton:
            adaptive_height: True
            adaptive_width: True
            id: apps
            icon: "cogs"
            md_bg_color: menubar.md_bg_color
            on_release: sm.current = 'APPLICATIONS'


        MDIconButton:
            adaptive_height: True
            adaptive_width: True
            icon: "form-select"
            md_bg_color: menubar.md_bg_color
            on_release: sm.current = 'Product'

        MDIconButton:
            adaptive_height: True
            adaptive_width: True
            icon: "drawing-box"
            md_bg_color: menubar.md_bg_color
            on_release: sm.current = 'Drawing'
        
        MDIconButton:
            adaptive_height: True
            adaptive_width: True
            icon: "note-check"
            md_bg_color: menubar.md_bg_color
            on_release: sm.current = 'Casecheck'
        
        MDIconButton:
            adaptive_height: True
            adaptive_width: True
            icon: "information"
            md_bg_color: menubar.md_bg_color
            on_release: sm.current = 'INFO'
        
        MDIconButton:
            id: colorp
            icon: "palette-advanced"
            text: "COLORS"
            md_bg_color: menubar.md_bg_color
            on_release: app.open_color_picker()
        
        MDIconButton:
            id: dater
            icon: "calendar-range"
            text: "DATE"
            md_bg_color: menubar.md_bg_color
            on_release: app.app_calendar()

        MDIconButton:
            id: timer
            icon: "clock-outline"
            text: "TIME"
            md_bg_color: menubar.md_bg_color
            on_release: app.app_timer()

    ScreenManager:
        id: sm
        size_hint: 1, 1
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        transition: 
        MDScreen:
            id: HOME
            name: "HOME"
            spacing: "12dp"
            MDLabel:
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                size_hint: 0.5, 0.8
                text: "Wellcome to the Open EQMS Demo! This app is a dev version ... something, it has an AI based on text input learning.\n\n\n Please feel free to explore..."
            


        MDScreen:
            id: applications
            name: "APPLICATIONS"
            #MDBottomAppBar:
             #   md_bg_color: app.theme_cls.primary_color
